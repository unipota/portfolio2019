{
  "name": "portfolio2019",
  "version": "1.0.0",
  "description": "unipota's portfolio website",
  "author": "unipota",
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "dev": "cross-env NUXT_HOST=0.0.0.0 NUXT_PORT=3000 nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "cross-env DEPLOY_ENV=GH_PAGES nuxt generate --modern=client",
    "deploy": "gh-pages -d dist -m 'circleci: update [skip ci]' -t"
  },
  "dependencies": {
    "@nuxtjs/pwa": "^3.0.0-beta.20",
    "nuxt": "^2.14.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^0.0.1",
    "@nuxtjs/eslint-module": "^0.0.1",
    "@nuxtjs/style-resources": "^0.1.2",
    "@tweenjs/tween.js": "^17.4.0",
    "awesome-sass-easing": "^1.1.2",
    "babel-eslint": "^10.1.0",
    "color": "^3.1.2",
    "cross-env": "^5.2.1",
    "detect-browser": "^4.8.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.1.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-nuxt": "^0.5.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "gh-pages": "^2.2.0",
    "node-sass": "^4.14.1",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "pug": "^3.0.1",
    "pug-plain-loader": "^1.0.0",
    "ress": "^1.2.2",
    "sass-loader": "^7.3.1",
    "vue-lazyload": "^1.3.3"
  },
  "browserslist": [
    "last 2 version",
    "android >= 4.4",
    "IE 11"
  ]
